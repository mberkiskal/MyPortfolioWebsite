@using MVCCV.Models.Entity;
@model List<TBLEXPERIENCE>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Experience List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Subtitle</th>
        <th>Date</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach(var item in Model)
    {
    <tr>
        <td>@item.ID</td>
        <td>@item.TITLE</td>
        <td>@item.SUBTITLE</td>
        <td>@item.DATE</td>
        <td> <a href="/Experience/DeleteExperience/@item.ID" class="btn btn-danger">Delete</a></td>
        <td> <a href="/Experience/BringExperience/@item.ID" class="btn btn-primary">Edit</a></td>
    </tr>
    }
</table>
<a href="/Experience/AddExperience/" class="btn btn-success">Yeni Deneyim Ekle</a>

