@model MVCCV.Models.Entity.TBLEXPERIENCE
@{
    ViewBag.Title = "BringExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Update Experience</h4>
<br />
<form class="form-group" method="post">
    <br />
    @Html.Label("Experience ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Title")
    @Html.TextBoxFor(x => x.TITLE, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Subtitle")
    @Html.TextBoxFor(x => x.SUBTITLE, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Description")
    @Html.TextAreaFor(x => x.DESCRIPTION, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Date")
    @Html.TextAreaFor(x => x.DATE, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>
</form>

