@model MVCCV.Models.Entity.TBLEDUCATION
@{
    ViewBag.Title = "BringEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Update Educations</h4>
<br />
<form class="form-group" method="post">
    <br />
    @Html.Label("Title")
    @Html.TextBoxFor(x => x.TITLE, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.TITLE, "", new { @style = "color:Red" })
    <br />
    @Html.Label("Subtitle1")
    @Html.TextBoxFor(x => x.SUBTITLE1, new { @class = "form-control" })
    <br />
    @Html.Label("Subtitle2")
    @Html.TextBoxFor(x => x.SUBTITLE2, new { @class = "form-control" })
    <br />
    @Html.Label("GPA")
    @Html.TextBoxFor(x => x.GPA, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GPA, "", new { @style = "color:Red" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.DATE, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>
</form>



